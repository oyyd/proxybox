{
  "name": "proxybox",
  "version": "1.0.0",
  "description": "Proxy tools basing on the SOCKS protocol and relative implementations.",
  "main": "lib/main_entry.js",
  "scripts": {
    "pack": "export CSC_IDENTITY_AUTO_DISCOVERY=false && build --dir",
    "build": "babel src --out-dir lib --source-maps inline",
    "dev": "babel src --out-dir lib --source-maps inline -w",
    "start": "electron ."
  },
  "author": "oyyd <oyydoibh@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "electron": "^1.6.10",
    "electron-builder": "^18.6.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "os-proxy": "^0.6.2",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "semantic-ui-react": "^0.68.5"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "eslintConfig": {
    "extends": "airbnb",
    "rules": {
      "semi": [
        2,
        "never"
      ],
      "import/no-extraneous-dependencies": 0,
      "import/prefer-default-export": 0,
      "react/jsx-filename-extension": 0,
      "react/prefer-stateless-function": 0
    },
    "env": {
      "browser": true
    }
  },
  "build": {
    "appId": "your.id",
    "mac": {
      "category": "your.app.category.type"
    }
  }
}
